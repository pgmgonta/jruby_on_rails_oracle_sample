development:
  adapter: oracle_enhanced
  host: hostname
  port: 1521
  database: xe
  username: user
  password: secret

# re-generated from your development database when you run "rake".
# Do not set this db to the same as development or production.
test:
  adapter: oracle_enhanced
  host: hostname
  port: 1521
  database: xe
  username: user
  password: secret

